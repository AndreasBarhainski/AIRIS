{
  "name": "airis",
  "version": "1.0.0",
  "description": "ComfyUI Workflows Frontend",
  "scripts": {
    "frontend": "cd frontend && npm run dev",
    "backend": "cd backend && node index.js",
    "dev": "concurrently \"npm run backend\" \"npm run frontend\"",
    "heroku-postbuild": "npm cache clean --force && cd frontend && npm install && npm run build && cd ../backend && npm install"
  },
